{
  "homepage": "https://Bonzar.github.io/pomodoro_box",
  "name": "pomodoro_box",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && yarn generate-pwa-assets && vite build && shx cp dist/index.html dist/404.html",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type": "tsc --noEmit",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator",
    "predeploy": "yarn type && yarn lint && yarn build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@csstools/postcss-global-data": "^1.0.3",
    "@reduxjs/toolkit": "^1.9.5",
    "postcss": "^8.4.24",
    "postcss-custom-media": "^9.1.3",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "sonner": "^0.4.0",
    "the-new-css-reset": "^1.8.4",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@types/ramda": "^0.29.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vite-pwa/assets-generator": "^0.0.2",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^5.0.0",
    "prettier": "2.8.8",
    "shx": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.3"
  }
}
